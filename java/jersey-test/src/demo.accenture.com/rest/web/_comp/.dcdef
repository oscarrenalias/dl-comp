<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>rest/web</name>
	<vendor>demo.accenture.com</vendor>
	<component-type>
		<vendor>sap.com</vendor>
		<type>J2EE</type>
		<sub-type>WebModule</sub-type>
	</component-type>
	<properties>
		<folder-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<folder-attribute attribute="web-inf" path="WebContent/WEB-INF"/>
		</folder-attributes>
		<j2ee-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<jee-version value="5.0"/>
		</j2ee-attributes>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/bi/bp/webmodule</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>web</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>rest/ext/lib</name>
				<vendor>demo.accenture.com</vendor>
			</dc-ref>
			<at-runtime/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>rest/ext/lib</name>
				<vendor>demo.accenture.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>rest/ext/lib</name>
				<vendor>demo.accenture.com</vendor>
			</dc-ref>
			<pp-ref>archives</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>engine.jee5.facade</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
	</dependencies>
	<folders>
		<source-folder>WebContent</source-folder>
		<package-folder>source</package-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="false">
			<sc-ref>
				<name>DemoComponents</name>
				<vendor>demo.accenture.com</vendor>
			</sc-ref>
		</grant>
	</access-control-list>
</development-component>
